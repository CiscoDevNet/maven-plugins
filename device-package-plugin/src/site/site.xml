<?xml version="1.0" encoding="UTF-8"?>
<project>

	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>${maven-fluido-skin.version}</version>
	</skin>
	
	<bannerLeft>
		<src>images/cisco-large.png</src>
    	<href>https://www.cisco.com/</href>
	</bannerLeft>

    <custom>
        <fluidoSkin>
            <gitHub>
                <projectId>hankolerd/maven-plugins</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>gray</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>

	<version position="right"/>
	<publishDate position="right"/>

    <body>

    	<breadcrumbs>
	    	<item name="Cisco" href="https://cisco.github.io"/>
	    	<item name="Maven Plugins"/>
      		<item name="${project.name}" href="${project.url}"/>
    	</breadcrumbs>
		
		<menu name="Overview">
			<item name="Introduction" href="index.html" />
			<item name="Goals" href="plugin-info.html"/>
			<item name="Usage" href="usage.html" />
			<item name="License" href="license.html" />
			<item name="Release Notes" href="changes-report.html" />
		</menu>
				
        <menu ref="reports"/>

        <menu name="Community">
        	<item name="Cisco DevNet" href="https://developer.cisco.com"/>
        	<item name="Cisco GitHub" href="https://cisco.github.io"/>
        </menu>
    </body>	

    <poweredBy>
    	<logo name="Cisco" href="https://www.cisco.com" img="images/cisco-small.png"/>
  	</poweredBy>

</project>